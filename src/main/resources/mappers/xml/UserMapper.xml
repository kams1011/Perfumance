<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="Kams.Perfumance.mapper.UserMapper">

    <select id="getList" resultType="Kams.Perfumance.vo.UserVo">
        SELECT * FROM user_info
    </select>

    <insert id="Insert">
        INSERT User_Info VALUES (#{id}, #{pwd}, #{nick}, #{email}, #{img}, #{regdt}, #{deldt}, #{dealnum});
    </insert>




</mapper>