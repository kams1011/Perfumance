<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="Kams.Perfumance.mapper.UserMapper">

    <select id="getList" resultType="Kams.Perfumance.vo.UserVo">
        SELECT * FROM user_info
    </select>

    <insert id="InsertUser"  useGeneratedKeys="true" keyProperty="uno">
        INSERT INTO user_info(id,pwd,nick,email,regdt)
        VALUE (#{id},#{pwd},#{nick},#{email},#{regdt})
    </insert>

    <select id="findByUserId" resultType="Kams.Perfumance.vo.UserVo">
        SELECT * FROM user_info WHERE id=#{id}
    </select>




</mapper>